{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "migrated",
	"description": "permissions that were migrated from v1",
	"local": true,
	"windows": ["main"],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"app:default",
		"resources:default",
		"menu:default",
		"tray:default",
		{
			"identifier": "http:default",
			"allow": [{ "url": "http://*" }, { "url": "https://*" }]
		}
	]
}
